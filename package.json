{
  "type": "module",
  "name": "@amitdeshmukh/ax-crew",
  "version": "5.0.0",
  "description": "Build and launch a crew of AI agents with shared state. Built with axllm.dev",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=21.0.0"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --outDir dist",

    "uuid": "^10.0.0"
  },
  "peerDependencies": {
    "@ax-llm/ax": "^14.0.36",
    "@ax-llm/ax-tools": "^14.0.36"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/big.js": "^6.2.2",
    "@types/node": "^20.19.21",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.0.9",
    "@vitest/ui": "^3.0.9",
    "typescript": "^5.5.3",
    "vitest": "^3.0.9"
  },
  "homepage": "https://github.com/amitdeshmukh/ax-crew#readme",
  "repository": {
